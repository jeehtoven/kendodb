
							if (q == 0)
							{						
								if (gcn_names == trim) 
								{
									r++;
									q++;
								}
							}
							else if (q == 1 && r == 1)
							{						
								cout << "Column: " << gcn_names << endl;
								r--;
								q == 2;
							}
							else if (q == 2)
							{
								cout << "Datatype: " << gcn_names << endl;
								q == 0;						
							}
